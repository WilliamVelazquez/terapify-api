{
  "name": "terapify-api",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@terapify-db-user",
    "DB_PASSWORD": "@terapify-db-password",
    "DB_HOST": "@terapify-db-host",
    "DB_NAME": "@terapify-db-name"
  }
}
